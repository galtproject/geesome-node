{
  "name": "geesome-node-ui",
  "version": "0.1.0",
  "scripts": {
    "start": "yarn && npm run dev",
    "dev": "copy-avm-public dist && ./node_modules/.bin/parcel ./index.html",
    "build": "yarn && rm -rf ./dist && copy-avm-public dist && node --max-old-space-size=8192 ./node_modules/.bin/parcel build ./index.html --dist-dir ./dist --reporter @parcel/reporter-bundle-analyzer && cp package.json ./dist/",
    "build-new-ver": "touch dist/package.json > /dev/null 2>&1; DIST_VERSION=$(node -p -e \"require('./dist/package.json').version\"); CUR_VERSION=$(node -p -e \"require('./package.json').version\"); if [ \"$DIST_VERSION\" != \"$CUR_VERSION\" ]; then npm run build; fi",
    "serve": "./node_modules/.bin/http-server ./dist",
    "postinstall": "copy-avm-public dist"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "author": "Galt Project Core Team",
  "license": "ISC",
  "dependencies": {
    "@galtproject/vue-locale": "0.1.10",
    "@swc/helpers": "^0.2.13",
    "async": "^2.6.4",
    "axios": "^0.21.2",
    "bluebird": "^3.5.2",
    "default-passive-events": "^1.0.7",
    "dexie": "^2.0.4",
    "ethers": "^3.0.27",
    "file-saver": "^2.0.2",
    "font-awesome": "https://github.com/FortAwesome/Font-Awesome",
    "geesome-libs": "https://github.com/galtproject/geesome-libs#9b25ebe",
    "geesome-vue-components": "https://github.com/galtproject/geesome-vue-components#2875185",
    "is-ipfs": "^0.4.7",
    "javascript-obfuscator": "^0.18.1",
    "js-yaml": "^3.13.1",
    "latlon-geohash": "^1.1.0",
    "lodash": "^4.17.21",
    "marked": "^4.0.10",
    "mediaelement": "^4.2.9",
    "mime": "^2.5.2",
    "moment": "^2.29.2",
    "object-hash": "^1.3.1",
    "p-iteration": "^1.1.7",
    "speed-measure-webpack-plugin": "^1.5.0",
    "terser": "^5.7.1",
    "url-search-params-polyfill": "^5.1.0",
    "vue": "2.6.11",
    "vue-drag-drop": "^0.2.3",
    "vue-material": "^1.0.0-beta-10.2",
    "vue-multiselect": "^2.1.0",
    "vue-notification": "^1.3.4",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.13",
    "vue2-editor": "^2.6.6",
    "vuex": "3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-syntax-bigint": "^7.8.3",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@parcel/reporter-bundle-analyzer": "^2.0.0-nightly.1694",
    "@parcel/transformer-image": "2.0.0-rc.0",
    "@parcel/transformer-sass": "2.0.0-rc.0",
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.2",
    "@types/node": "^8.10.17",
    "autoprefixer": "^10.3.6",
    "buffer": "^5.1.0",
    "chai": "^4.1.2",
    "dotenv": "^6.0.0",
    "http-server": "^0.10.0",
    "mocha": "^5.2.0",
    "node-sass": "7.0.0",
    "parcel": "^2.0.0-rc.0",
    "postcss": "^8.1.0",
    "tslint": "^5.18.0",
    "typescript": "^4.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.common.js"
  }
}
