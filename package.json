{
  "name": "geesome-node",
  "version": "0.3.0",
  "description": "",
  "main": "index.ts",
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "yarn --no-optional -W && DEBUG=geesome* ./node_modules/.bin/ts-node ./index.ts",
    "in-docker-start": "cd /geesome-node && yarn --no-optional -W && cp -R /geesome-node/node_modules/@geesome/ui/* /geesome-node/frontend/docker-dist/ && npm run migrate-database; npm run ssg-migrate-database; npm run sci-migrate-database; npm run group-migrate-database; npm run start",
    "recreate-database": "dropdb geesome_node && createdb geesome_node",
    "recreate-test-database": "dropdb geesome_test && createdb geesome_test",
    "migrate-database": "( cd app/modules/database && ../../../node_modules/.bin/ts-node ./writeMigrationConfig.ts && NODE_ENV=production ../../../node_modules/.bin/sequelize db:migrate)",
    "create-migration": "./node_modules/.bin/sequelize migration:generate --migrations-path app/modules/socNetImport/migrations --name",
    "group-migrate-database": "( cd app/modules/group && ../../../node_modules/.bin/ts-node ./writeMigrationConfig.ts && NODE_ENV=production ../../../node_modules/.bin/sequelize db:migrate)",
    "group-create-migration": "./node_modules/.bin/sequelize migration:generate --migrations-path app/modules/group/migrations --name",
    "ssg-migrate-database": "( cd app/modules/staticSiteGenerator && ../../../node_modules/.bin/ts-node ./writeMigrationConfig.ts && NODE_ENV=production ../../../node_modules/.bin/sequelize db:migrate)",
    "ssg-create-migration": "./node_modules/.bin/sequelize migration:generate --migrations-path app/modules/staticSiteGenerator/migrations --name",
    "sci-migrate-database": "( cd app/modules/socNetImport && ../../../node_modules/.bin/ts-node ./writeMigrationConfig.ts && NODE_ENV=production ../../../node_modules/.bin/sequelize db:migrate)",
    "sci-create-migration": "./node_modules/.bin/sequelize migration:generate --migrations-path app/modules/socNetImport/migrations --name",
    "rollback-migration": "( cd app/modules/database && ../../../node_modules/.bin/ts-node ./writeMigrationConfig.ts && NODE_ENV=production ../../../node_modules/.bin/sequelize db:migrate:undo)",
    "check-perfomance": "npm run recreate-test-database; rm -rf ./.jsipfs-test; ts-node check/geesomePerfomance.ts --trace_gc",
    "check-client-perfomance": "node --max-old-space-size=4096 -- node_modules/.bin/ts-node -P tsconfig.json check/geesomeClientPerfomance.ts",
    "generate-docs": "./node_modules/.bin/apidoc -i app/modules/api -o docs -t node_modules/apidoc-template/template && ./node_modules/.bin/http-server ./docs",
    "test": "rm -rf database-test.sqlite; rm -rf test-data/*.sqlite; rm -rf ./.jsipfs-test; GATEWAY_PORT=2083 PORT=7772 SSG_RUNTIME=1 DEBUG=geesome* DATA_DIR=test-data ./node_modules/.bin/mocha --exit -r ts-node/register 'test/**/*.test.ts' -t 10000",
    "testCoverage": "./node_modules/.bin/nyc -r lcov -e .ts -x \"*.test.ts\" ./node_modules/.bin/mocha -r ts-node/register test/**/*.test.ts && nyc report",
    "docker": "STORAGE_STAGING=$PWD/.docker-data/ipfs-staging STORAGE_DATA=$PWD/.docker-data/ipfs GEESOME_DATA=$PWD/.docker-data/geesome-data GEESOME_FRONTEND_DIST=$PWD/.docker-data/geesome-frontend docker-compose up",
    "docker-build": "bash/docker-build.sh",
    "docker-upgrade": "bash/docker-rebuild-and-upgrade.sh"
  },
  "author": "",
  "license": "ISC",
  "workspaces": [
    "./app/modules/staticSiteGenerator/site",
    "./app/modules/filecoinStore/store"
